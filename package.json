{
  "name": "@codpoe/iconfont",
  "version": "0.0.3",
  "description": "Generator of webfonts from svg icons",
  "main": "lib/index.js",
  "scripts": {
    "bootstrap": "yarn || npm",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run bootstrap && npm run build:js && npm run build:types",
    "build:js": "npx babel src --out-dir lib --extensions .ts --source-maps inline",
    "build:types": "npx tsc --emitDeclarationOnly",
    "test": "npx jest",
    "test:temp": "node test/manual.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Codpoe/iconfont.git"
  },
  "keywords": [
    "font",
    "webfont",
    "svg",
    "ttf",
    "woff",
    "woff2"
  ],
  "author": "codpoe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Codpoe/iconfont/issues"
  },
  "dependencies": {
    "glob": "^7.1.4",
    "handlebars": "^4.0.5",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "svg2ttf": "^4.2.0",
    "svgicons2svgfont": "^9.0.4",
    "ttf2eot": "^2.0.0",
    "ttf2woff": "^2.0.1",
    "ttf2woff2": "^2.0.3",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.6",
    "@types/lodash": "^4.14.121",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^11.11.6",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.1.0",
    "prettier": "^1.17.1"
  }
}
